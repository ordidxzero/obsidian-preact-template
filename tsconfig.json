{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "src",
    // "inlineSourceMap": true,
    // "inlineSources": true,
    "jsx": "react",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "module": "ESNext",
    "target": "esnext",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "lib": ["dom", "esnext"]
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
